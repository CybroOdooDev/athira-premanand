<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_team_leader" model="ir.ui.view">
        <field name="model">team.leader</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" string="Name"/>
            </tree>
        </field>
    </record>
    <record id="view_team_leader_form" model="ir.ui.view">
        <field name="model">team.leader</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name" string="Name"/>
                        </group>
                        <group>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="team_leader_action" model="ir.actions.act_window">
        <field name="name">Team Leader</field>
        <field name="res_model">team.leader</field>
        <field name="view_mode">tree,form</field>
        <field name="type">ir.actions.act_window</field>
    </record>
</odoo>
