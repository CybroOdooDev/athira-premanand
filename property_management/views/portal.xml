<odoo>
    <template id="portal_my_property_owners" name="Property Owners" customize_show="True" inherit_id="portal.portal_my_home"
              priority="20">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Property Owner</t>
                <t t-set="url" t-value="'/my/properties'"/>
                <t t-set="placeholder_count" t-value="'property_count'"/>
            </t>
        </xpath>
    </template>
    <template id="portal_commission_history" name="Commission History" customize_show="True" inherit_id="portal.portal_my_home"
              priority="20">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Commission History</t>
                <t t-set="url" t-value="'/my/commission-history'"/>
                <t t-set="placeholder_count" t-value="'commission_history_count'"/>
            </t>
        </xpath>
    </template>
</odoo>